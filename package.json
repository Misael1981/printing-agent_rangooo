{
  "name": "printing-agent_rangooo",
  "version": "1.0.2",
  "description": "Agente de impress√£o local do Rangooo",
  "author": "Misael Borges",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.rangooo.agente",
    "publish": [
      {
        "provider": "github",
        "owner": "https://github.com/Misael1981",
        "repo": "https://github.com/Misael1981/printing-agent_rangooo.git"
      }
    ],
    "productName": "Agente de Impressao Rangooo",
    "directories": {
      "output": "dist"
    },
    "extraResources": [
      ".env"
    ],
    "win": {
      "target": "nsis",
      "icon": "src/public/assets/favicon.ico"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false
    },
    "npmRebuild": false
  },
  "dependencies": {
    "auto-launch": "^5.0.6",
    "dotenv": "^17.2.3",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.6.2",
    "node-thermal-printer": "^4.5.0",
    "serialport": "^13.0.0",
    "winston": "^3.19.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "electron": "^33.2.1",
    "electron-builder": "^26.0.12"
  }
}
